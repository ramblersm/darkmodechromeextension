{
    "manifest_version": 3,
    "name": "Dark Mode Toggle",
    "version": "1.0",
    "description": "Toggle dark mode on any website.",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }
  